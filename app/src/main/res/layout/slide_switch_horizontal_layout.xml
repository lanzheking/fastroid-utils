	<!--android:layout_width="94dp"-->
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="60dp"
	android:layout_height="37dip">
	<View
		android:id="@+id/ViewOff"
		android:layout_width="56dp"
		android:layout_height="37dp"
		android:background="@drawable/slideswitch_bg_off"
		android:layout_alignParentRight="true">
	</View>
	<View
		android:id="@+id/ViewOn"
		android:layout_width="56dp"
		android:layout_height="37dp"
		android:background="@drawable/slideswitch_bg_on">
	</View>

	<Button
		android:id="@+id/ButtonBar"
		android:background="@drawable/slideswitch_bar"
		android:layout_width="41dip"
		android:layout_height="37dip">
	</Button>

	<View
		android:id="@+id/ViewBg"
		android:layout_width="94dp"
		android:layout_height="37dp"
		android:background="@drawable/slideswitch_bg">
	</View>

</RelativeLayout>
